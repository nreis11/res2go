<a id="home" class="nav" href='/reservoirs'>Res2Go</a>
<% if current_user %>
  <a class="nav" href='/reservoirs'>All Reservoirs</a>
  <a class="nav" href="/users/<%= current_user.id %> ">My Reservoirs</a>
  <form id="logout-button" action="/sessions" method="POST">
    <input type="hidden" name="_method" value="DELETE">
    <input class="session button" type="submit" value="Logout">
  </form>
<% else %>
  <a id="login-button" class="nav" href="/sessions/new">Login</a>
  <a id="register-button" class="nav" href="/users/new">Sign Up</a>
<% end %>
